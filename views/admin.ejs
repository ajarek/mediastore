<%-include('./partials/header')%>
<%-include('./partials/navLogin')%>

<div class="container p-4 ">
    <h1>Panel admina</h1>
    <h1 class="mb-3">New Mobile</h1>
  <form class="row g-3" action="/admin/" method="POST" >
      <div class="col-md-12 ">
          <label for="title" class="form-label">Title</label>
          <input type="text" class="form-control"  name="title" required>
        </div>
        <div class="col-md-12">
            <label for="photo" class="form-label">Photo</label>
            <input type="url" class="form-control" name="photo" placeholder="https://picsum.photos/400/400">
          </div>
      <div class="col-md-12">
          <label for="desc1" class="form-label">Description1</label>
          <input type="text" class="form-control"  name="desc1" required>
        </div>
        <div class="col-md-12">
            <label for="desc2" class="form-label">Description2</label>
            <input type="text" class="form-control"  name="desc2" required>
          </div>
          <div class="col-md-12">
            <label for="desc3" class="form-label">Description3</label>
            <input type="text" class="form-control"  name="desc3" required>
          </div>
          <div class="col-md-12">
            <label for="desc4" class="form-label">Description4</label>
            <input type="text" class="form-control"  name="desc4" required>
          </div>
          <div class="col-md-12">
            <label for="desc5" class="form-label">Description5</label>
            <input type="text" class="form-control"  name="desc5" required>
          </div>
          <div class="col-md-12">
            <label for="desc6" class="form-label">Description6</label>
            <input type="text" class="form-control"  name="desc6" required>
          </div>
        
        <div class="col-md-12">
          <label for="price" class="form-label">Price</label>
          <input type="text" class="form-control" name="price" required>
        </div>
        
      <div class="col-12">
        <button type="submit" class="btn btn-warning">Create a Mobile</button>
      </div>
    </form>
  </div>

<%-include('./partials/footer')%>